namespace %PROJECT_NAME%.Utils
{
    public class Platform
    {
        public enum Type
        {
            Editor,
            Android,
            iOS
        }

        public static TYPE GetType()
        {
#if UNITY_EDITOR
            return Type.Editor;
#elif UNITY_ANDROID
            return Type.Android;
#elif UNITY_IOS
            return Type.iOS;
#endif
            return Type.Editor;
        }
    }
}